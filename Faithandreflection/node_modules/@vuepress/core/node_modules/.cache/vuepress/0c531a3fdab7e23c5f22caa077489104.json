n 'LinkedinIcon'\n        case 'youtube':\n          return 'YoutubeIcon'\n        case 'phone':\n          return 'PhoneIcon'\n        default:\n          return ''\n      }\n    },\n  },\n}\n</script>\n\n<style lang=\"stylus\" scoped>\nol, ul {\n  list-style: none;\n  margin: 0;\n  padding: 0;\n}\n\n.footer {\n  height: 60px;\n  box-sizing: border-box;\n  background: linear-gradient(to right, darken($accentColor, 50%), darken($secondaryColor, 70%));\n  color: #FFF;\n  display: flex;\n  padding: 15px 32px;\n  word-break: wrap;\n\n  .footer-left-wrap {\n    line-height: 30px;\n    flex: 1;\n    display: flex;\n\n    .contact {\n      display: flex;\n\n      .contact-item {\n        flex: 1;\n        margin-right: 10px;\n\n        a {\n          font-size: 12px;\n          color: rgba(255, 255, 255, 0.45);\n          text-decoration: none;\n          transition: color 0.3s;\n\n          &:hover {\n            color: #FFF;\n          }\n        }\n      }\n    }\n  }\n\n  .footer-right-wrap {\n    flex: 1;\n    display: none;\n    align-items: center;\n\n    @media (min-width: $MQNarrow) {\n      display: flex;\n      justify-content: flex-end;\n    }\n\n    .copyright {\n      display: flex;\n      justify-content: flex-end;\n\n      .copyright-item {\n        flex: 0 0 auto;\n        padding: 0 10px;\n        position: relative;\n        line-height: 12px;\n\n        &:last-child {\n          border-right: none;\n        }\n\n        a {\n          font-size: 12px;\n          color: rgba(255, 255, 255, 0.6);\n          text-decoration: none;\n          transition: color 0.3s;\n\n          &:hover {\n            color: rgba(255, 255, 255, 0.9);\n          }\n        }\n      }\n    }\n  }\n}\n\n@media (max-width: $MQMobile) {\n  .footer {\n    height: 100px;\n    flex-direction: column;\n\n    .footer-left-wrap {\n      align-items: center;\n      justify-content: center;\n    }\n  }\n}\n</style>\n"]}]}